	<form name="form1" action="index.jsp">
		<div align="center">

			<script type="text/javascript">
			
			
				function mayor(array) {

					var mayor = 0;
					for (var i = 0; i < array.length; i++) {
						if (array[i] > mayor) {
							mayor = array[i];
						}
					}

					return mayor;
				}
			</script>

