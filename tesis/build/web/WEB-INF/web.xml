<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>genericQuery</servlet-name>
        <servlet-class>com.genericQuery</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PersonaDAO</servlet-name>
        <servlet-class>com.PersonaDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrivilegioDAO</servlet-name>
        <servlet-class>com.PrivilegioDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RecibeArchivo</servlet-name>
        <servlet-class>com.RecibeArchivo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExportaInfo</servlet-name>
        <servlet-class>com.ExportaInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletUtil</servlet-name>
        <servlet-class>com.ServletUtil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MapaDAO</servlet-name>
        <servlet-class>com.MapaDAO</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>genericQuery</servlet-name>
        <url-pattern>/com.genericQuery</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PersonaDAO</servlet-name>
        <url-pattern>/com.PersonaDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrivilegioDAO</servlet-name>
        <url-pattern>/com.PrivilegioDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RecibeArchivo</servlet-name>
        <url-pattern>/com.RecibeArchivo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExportaInfo</servlet-name>
        <url-pattern>/com.ExportaInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletUtil</servlet-name>
        <url-pattern>/com.ServletUtil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MapaDAO</servlet-name>
        <url-pattern>/com.MapaDAO</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
